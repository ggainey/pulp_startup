if [ -z "${ROOT}" ]; then
  ROOT=/home/ggainey/github/Pulp3
fi
REPOS="$@"
if [ -z "${REPOS}" ]; then
  REPOS="pulpcore pulp_file pulp_rpm pulp_container pulp-certguard pulp_deb pulp-cli"
fi
echo ${REPOS}
for r in ${REPOS}; do
  echo "${ROOT}/${r}"
  cd "${ROOT}/${r}"; git checkout main; git fetch upstream; git rebase upstream/main; git push origin main
done
