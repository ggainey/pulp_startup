export ROOT=/home/ggainey/github/sat6-11
REPO_LIST="pulpcore,3.16 pulp_file,1.10 pulp_rpm,3.17 pulp_container,2.9 pulp-certguard,1.5 pulp-smash,main"
for rv in $REPO_LIST
do
  OLDIFS=$IFS
  IFS=","
  set -- $rv
  echo REPO $1 VERSION $2
  update-all $1
  cd $ROOT/$1; git checkout $2; git rebase upstream/$2; echo
  IFS=$OLDIFS
done
